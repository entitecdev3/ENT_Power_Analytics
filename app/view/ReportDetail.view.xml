<mvc:View
  controllerName="entitec.pbi.embedding.controller.ReportDetail"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:core="sap.ui.core"
  xmlns="sap.m"
  xmlns:html="http://www.w3.org/1999/xhtml">

  <Page showHeader="false">
    <subHeader>
      <core:Fragment fragmentName="entitec.pbi.embedding.fragments.SubHeaderToolbar" type="XML" />
    </subHeader>
    <content>
      <VBox class="sapUiSmallMargin">
        <html:iframe id="embedHTML" width="100%" height="600px" frameborder="0" />
      </VBox>
    </content>
  </Page>
</mvc:View>
