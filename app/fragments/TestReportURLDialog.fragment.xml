<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">

  <Dialog title="Test Report URL" contentWidth="35rem" resizable="true" draggable="true" class="sapUiResponsivePadding--header sapUiResponsivePadding--content sapUiResponsivePadding--footer">

    <VBox class="sapUiSmallMargin">
      <Label text="Paste Power BI Report URL to Test" />
      <Input id="testURLInput" placeholder="https://app.powerbi.com/groups/00000000-0000-0000-0000-000000000000/reports/11111111-1111-1111-1111-111111111111/ReportSection0000000000000000000000" width="100%" valueHelpIconSrc="sap-icon://paste" showValueHelp="true" valueHelpRequest=".onPasteExampleURL" />

      <HBox class="sapUiTinyMarginTop"></HBox>
    </VBox>

    <beginButton>
      <Button text="Load Report" icon="sap-icon://inspection" type="Emphasized" press=".onTestURLLoad" class="sapUiTinyMarginBegin" />
    </beginButton>

    <endButton>z
      <Button text="Close" press=".onTestUrlDialogClose" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>