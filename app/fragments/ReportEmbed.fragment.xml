<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Dialog
    id="reportDialog"
    title="Power BI Report"
    contentWidth="80%"
    contentHeight="80%"
    resizable="true"
    draggable="true"
    afterClose=".onDialogClose">
    
    <content>
      <core:HTML id="reportEmbedHTML" />
    </content>

    <endButton>
      <Button text="Close" press=".onDialogClose" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>
